<% layout("/layouts/boilerplate") %> <%= success %>
<body>

    <h3>All Listings</h3>

    <div class="row row-cols-lg-3  row-cols-md-2 row-cols-sm-1">
   

    <% for(let listing of allListings ) { %>
        <div class="card col" >
            <a href ="/listings/<%= listing._id %>" class="listing-link">

            <img src="<%= listing.image.url %>" class="card-img-top" alt="listing_im age" style="height: 20rem"/>

            <div class="card-img-overlay">a</div>
            <div class="card-body">
                <h5 class="card-title"><%= listing.title %></h5>
                <p class="card-text">
                  <b>  <%= listing.description %></b><br/>
                &#8377;    <%= listing.price %> / night
             

                </p>
            </a>
            </div>
        </div>
    </a>
    <% } %>

</div>
    
    



 